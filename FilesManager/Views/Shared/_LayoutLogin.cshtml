<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FilesManager</title>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/scripts/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="~/scripts/css/style.css" type="text/css" rel="stylesheet" media="screen,projection" />
</head>
<body>
    <div class="container body-content">
        @RenderBody()

        @RenderSection("Scripts", required: false)
    </div>

    <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="~/scripts/js/materialize.js"></script>
    <script src="~/scripts/js/init.js"></script>
</body>
</html>
